<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="clip_vertical|start"
    android:orientation="vertical"
    tools:context=".AddObjectFragment">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/titleLabel"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="93dp" />

    <EditText
        android:id="@+id/edit_title"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="150dp"
        android:autofillHints="" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/urgencyLabel"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="242dp" />

    <Spinner
        android:id="@+id/spinner_urgency"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:theme="@null"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="261dp" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoryLabel"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="337dp" />

    <Spinner
        android:id="@+id/spinner_category"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:theme="@null"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/aboutLabel"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="502dp" />

    <EditText
        android:id="@+id/edit_about"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:importantForAutofill="no"
        android:inputType="textMultiLine|text"
        android:labelFor="@id/textView5"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="531dp" />

    <EditText
        android:id="@+id/edit_lat"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:importantForAutofill="no"
        android:inputType="textMultiLine|text"
        android:labelFor="@id/textView5"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="531dp" />

    <EditText
        android:id="@+id/edit_lon"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:importantForAutofill="no"
        android:inputType="textMultiLine|text"
        android:labelFor="@id/textView5"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="531dp" />


    <Button
        android:id="@+id/button_getCoords"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Get coordinates" />


    <Button
        android:id="@+id/button_addObject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Button" />

</LinearLayout>